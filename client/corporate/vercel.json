{
  "version": 2,
  "buildCommand": "npm ci && npm run build:static",
  "outputDirectory": "out",
  "framework": null,
  "routes": [
    {
      "src": "/hizmetler/([^/]+)/?",
      "dest": "/hizmetler/$1/index.html"
    },
    {
      "src": "/projeler/([^/]+)/?",
      "dest": "/projeler/$1/index.html"
    },
    {
      "src": "/blog/([^/]+)/?",
      "dest": "/blog/$1/index.html"
    },
    {
      "src": "/gizlilik-politikasi/?",
      "dest": "/gizlilik-politikasi/index.html"
    },
    {
      "src": "/cerez-politikasi/?",
      "dest": "/cerez-politikasi/index.html"
    },
    {
      "src": "/kvkk-aydinlatma-metni/?",
      "dest": "/kvkk-aydinlatma-metni/index.html"
    },
    {
      "src": "/site-haritasi/?",
      "dest": "/site-haritasi/index.html"
    },
    {
      "src": "/iletisim/?",
      "dest": "/iletisim/index.html"
    },
    {
      "src": "/hakkimizda/?",
      "dest": "/hakkimizda/index.html"
    },
    {
      "src": "/referanslar/?",
      "dest": "/referanslar/index.html"
    },
    {
      "src": "/sss/?",
      "dest": "/sss/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
